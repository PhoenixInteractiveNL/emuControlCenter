style "default"
{
  
  GtkRange       ::trough_border     = 2
  GtkPaned       ::handle_size       = 6
  GtkRange       ::slider_width      = 10
  GtkScrollbar   ::min_slider_length = 32
  GtkScrollbar	 ::slider_width		= 16
  GtkScrollbar	::stepper_size	= 16
  GtkCheckButton ::indicator_size    = 14
  GtkRadioButton::indicator_size	= 14
  GtkMenuBar     ::internal-padding  = 0
  GtkTreeView    ::expander_size     = 14
  GtkExpander    ::expander_size     = 16

  xthickness = 1
  ythickness = 1

  

  	fg[NORMAL]      	= "#000000"
	fg[PRELIGHT]     	= "#000000"
	fg[ACTIVE]        	= "#131313"
	fg[SELECTED]     	= "#ffffff"
	fg[INSENSITIVE]  	= "#9c9c9c"

	bg[NORMAL]    		= "#D4d4d9"
	bg[PRELIGHT]    	= "#c9c9c9"
	bg[ACTIVE]       	= "#c5c5c9"
	bg[SELECTED]   		= "#50586A"
	bg[INSENSITIVE]		= "#e3e3e3"
	
	base[NORMAL] 		= "#efeff6"
	base[PRELIGHT] 		= "#fefefe"
	base[ACTIVE]   		= "#fcfcfc"
	base[SELECTED] 		= "#92a1b5"
	base[INSENSITIVE] 	= "#fcfcfc"
	
	text[NORMAL]   		= "#000000"
	text[PRELIGHT]   	= "#000000"
	text[ACTIVE]     	= "#292929"
	text[SELECTED]  	= "#ffffff"
	text[INSENSITIVE] 	= "#6c6c6c"
	

}

style "default"  {
  engine "pixmap"
  {
 
    
    image
    {
      function          	= BOX
      shadow            	= IN
      file              	= "null.svg"
      border           	= { 1, 1, 1, 1 }
      stretch           	= TRUE
    }
    image
    {
      function          	= BOX
      shadow            	= OUT
      file              	= "null.svg"
      border            	= { 1, 1, 1, 1 }
      stretch           	= TRUE
    }
  
    
    
    image
    {
      function			= FOCUS
      file				= "null.svg"
      border			= { 6, 6, 6, 6 }
      stretch				= TRUE
    }	
    image
    {
      function			= ARROW
      state				= NORMAL
      file				= "arrow-up.png"
      border	= { 0, 0, 0, 0 }
      stretch	= TRUE
      arrow_direction	= UP
    }
    image
    {
      function		= ARROW
      state				= PRELIGHT
      file		= "arrow-up-pre.png"
      border	= { 0, 0, 0, 0 }
      stretch	= TRUE
      arrow_direction	= UP
    }
    image
    {
      function		= ARROW
      state				= INSENSITIVE
