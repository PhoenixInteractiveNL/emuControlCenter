<?xml version="1.0" encoding="utf-8"?>
<!--Joystick settings and mapping-->
<Configuration>
  <Settings>
    <Interface>DirectInput</Interface>
    <NumOfDevices>1</NumOfDevices>
    <Device0></Device0>
    <ButtonsPollingRate>10</ButtonsPollingRate>
    <AxesPollingRate>10</AxesPollingRate>
    <X_From>25000</X_From>
    <X_To>40000</X_To>
    <Y_From>25000</Y_From>
    <Y_To>40000</Y_To>
    <Z_From>25000</Z_From>
    <Z_To>40000</Z_To>
    <ARx_From>25000</ARx_From>
    <ARx_To>40000</ARx_To>
    <ARy_From>25000</ARy_From>
    <ARy_To>40000</ARy_To>
    <ARz_From>25000</ARz_From>
    <ARz_To>40000</ARz_To>
    <AX_From>25000</AX_From>
    <AX_To>40000</AX_To>
    <AY_From>25000</AY_From>
    <AY_To>40000</AY_To>
    <AZ_From>25000</AZ_From>
    <AZ_To>40000</AZ_To>
    <FRx_From>25000</FRx_From>
    <FRx_To>40000</FRx_To>
    <FRy_From>25000</FRy_From>
    <FRy_To>40000</FRy_To>
    <FRz_From>25000</FRz_From>
    <FRz_To>40000</FRz_To>
    <FX_From>25000</FX_From>
    <FX_To>40000</FX_To>
    <FY_From>25000</FY_From>
    <FY_To>40000</FY_To>
    <FZ_From>25000</FZ_From>
    <FZ_To>40000</FZ_To>
    <Rx_From>25000</Rx_From>
    <Rx_To>40000</Rx_To>
    <Ry_From>25000</Ry_From>
    <Ry_To>40000</Ry_To>
    <Rz_From>25000</Rz_From>
    <Rz_To>40000</Rz_To>
    <VRx_From>25000</VRx_From>
    <VRx_To>40000</VRx_To>
    <VRy_From>25000</VRy_From>
    <VRy_To>40000</VRy_To>
    <VRz_From>25000</VRz_From>
    <VRz_To>40000</VRz_To>
    <VX_From>25000</VX_From>
    <VX_To>40000</VX_To>
    <VY_From>25000</VY_From>
    <VY_To>40000</VY_To>
    <VZ_From>25000</VZ_From>
    <VZ_To>40000</VZ_To>
    <U_From>25000</U_From>
    <U_To>40000</U_To>
    <V_From>25000</V_From>
    <V_To>40000</V_To>
    <Sensitivity1AxisX>30</Sensitivity1AxisX>
    <Sensitivity1AxisY>30</Sensitivity1AxisY>
    <Sensitivity2AxisX>1</Sensitivity2AxisX>
    <Sensitivity2AxisY>1</Sensitivity2AxisY>
    <Sound>0</Sound>
    <ExtEnter>0</ExtEnter>
    <Toggles>0</Toggles>
    <ExclusiveMode>True</ExclusiveMode>
    <HandleFailures>True</HandleFailures>
  </Settings>
  <Mapping>
    <Item0>
      <Src>Joystick0 Axis Z</Src>
      <Dest>Mouse Axis X</Dest>
      <Action>Move mouse by X</Action>
    </Item0>
    <Item1>
      <Src>Joystick0 Axis Rz</Src>
      <Dest>Mouse Axis Y</Dest>
      <Action>Move mouse by Y</Action>
    </Item1>
    <Item2>
      <Src>Joystick0 Axis Ry [-]</Src>
      <Dest>Mouse Left</Dest>
      <Action>Left mouse click</Action>
    </Item2>
    <Item3>
      <Src>Joystick0 Axis Rx [-]</Src>
      <Dest>Mouse Right</Dest>
      <Action>Right mouse click</Action>
    </Item3>
    <Item4>
      <Src>Joystick0 Button 3</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>New Tab</Action>
    </Item4>
    <Item5>
      <Src>Joystick0 Button 3</Src>
      <Dest>Keyboard T</Dest>
      <Action>New Tab</Action>
    </Item5>
    <Item6>
      <Src>Joystick0 Button 1</Src>
      <Dest>Keyboard LWin</Dest>
      <Action>Show Desktop</Action>
    </Item6>
    <Item7>
      <Src>Joystick0 Button 1</Src>
      <Dest>Keyboard D</Dest>
      <Action>Show Desktop</Action>
    </Item7>
    <Item8>
      <Src>Joystick0 Button 0</Src>
      <Dest>Keyboard Menu Left</Dest>
      <Action>Previous Page</Action>
    </Item8>
    <Item9>
      <Src>Joystick0 Button 0</Src>
      <Dest>Keyboard Left</Dest>
      <Action>Previous Page</Action>
    </Item9>
    <Item10>
      <Src>Joystick0 Button 2</Src>
      <Dest>Keyboard Menu Left</Dest>
      <Action>Next Page</Action>
    </Item10>
    <Item11>
      <Src>Joystick0 Button 2</Src>
      <Dest>Keyboard Right</Dest>
      <Action>Next Page</Action>
    </Item11>
    <Item12>
      <Src>Joystick0 Axis X [-]</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Previous Tab</Action>
    </Item12>
    <Item13>
      <Src>Joystick0 Axis X [-]</Src>
      <Dest>Keyboard ShiftKey Left</Dest>
      <Action>Previous Tab</Action>
    </Item13>
    <Item14>
      <Src>Joystick0 Axis X [-]</Src>
      <Dest>Keyboard Tab</Dest>
      <Action>Previous Tab</Action>
    </Item14>
    <Item15>
      <Src>Joystick0 Axis X [+]</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Next Tab</Action>
    </Item15>
    <Item16>
      <Src>Joystick0 Axis X [+]</Src>
      <Dest>Keyboard Tab</Dest>
      <Action>Next Tab</Action>
    </Item16>
    <Item17>
      <Src>Joystick0 Pov Up</Src>
      <Dest>Mouse Wheel Up</Dest>
      <Action>Scroll Up</Action>
    </Item17>
    <Item18>
      <Src>Joystick0 Pov Down</Src>
      <Dest>Mouse Wheel Down</Dest>
      <Action>Scroll Down</Action>
    </Item18>
    <Item19>
      <Src>Joystick0 Button 5</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Zoom In</Action>
    </Item19>
    <Item20>
      <Src>Joystick0 Button 5</Src>
      <Dest>Keyboard Add</Dest>
      <Action>Zoom In</Action>
    </Item20>
    <Item21>
      <Src>Joystick0 Button 4</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Zoom Out</Action>
    </Item21>
    <Item22>
      <Src>Joystick0 Button 4</Src>
      <Dest>Keyboard Subtract</Dest>
      <Action>Zoom Out</Action>
    </Item22>
    <Item23>
      <Src>Joystick0 Button 11</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Close Current Tab</Action>
    </Item23>
    <Item24>
      <Src>Joystick0 Button 11</Src>
      <Dest>Keyboard W</Dest>
      <Action>Close Current Tab</Action>
    </Item24>
    <Item25>
      <Src>Joystick0 Pov Left</Src>
      <Dest>Keyboard Left</Dest>
      <Action>Scroll Left</Action>
    </Item25>
    <Item26>
      <Src>Joystick0 Pov Right</Src>
      <Dest>Keyboard Right</Dest>
      <Action>Scroll Right</Action>
    </Item26>
    <Item27>
      <Src>Joystick0 Button 8</Src>
      <Dest>Keyboard F5</Dest>
      <Action>Reload</Action>
    </Item27>
    <Item28>
      <Src>Joystick0 Button 9</Src>
      <Dest>Keyboard Menu Left</Dest>
      <Action>Home Page</Action>
    </Item28>
    <Item29>
      <Src>Joystick0 Button 9</Src>
      <Dest>Keyboard Home</Dest>
      <Action>Home Page</Action>
    </Item29>
    <Item30>
      <Src>Joystick0 Button 10</Src>
      <Dest>Keyboard ControlKey Left</Dest>
      <Action>Bookmarks Manager</Action>
    </Item30>
    <Item31>
      <Src>Joystick0 Button 10</Src>
      <Dest>Keyboard ShiftKey Left</Dest>
      <Action>Bookmarks Manager</Action>
    </Item31>
    <Item32>
      <Src>Joystick0 Button 10</Src>
      <Dest>Keyboard O</Dest>
      <Action>Bookmarks Manager</Action>
    </Item32>
  </Mapping>
</Configuration>