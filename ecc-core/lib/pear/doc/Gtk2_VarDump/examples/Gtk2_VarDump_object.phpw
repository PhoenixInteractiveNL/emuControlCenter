<?php
/**
*   Gtk2_VarDump example: Display an object
*
*   @author Christian Weiske <cweiske@php.net>
*/
require_once('Gtk2/VarDump.php');

$obj = null;
$obj->string = 'bla';
$obj->int    = 4;
$obj->float  = 2.4;
$obj->age    = 42;

$obj->child       = null;
$obj->child->name = 'kid';
$obj->child->age  = 23;

$obj->array = array('Tell', 'me', 'something', 'new');

new Gtk2_VarDump($obj, 'obj');
?>