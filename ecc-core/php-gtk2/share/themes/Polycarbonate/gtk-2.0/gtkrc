	{
            function        		= CHECK
            recolorable     		= TRUE
	    	  state 				= INSENSITIVE
            shadow          		= IN
            overlay_file   		= "check-ok-ins.svg"
            overlay_stretch 	= FALSE
        }
image 
	{
          function        		= FLAT_BOX
          recolorable     		= TRUE
      	stretch				= TRUE
          file            			= "null.svg"
          border          		= { 6, 4, 6, 4 }
        }

	
    }
}


