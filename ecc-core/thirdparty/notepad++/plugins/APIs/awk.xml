<?xml version="1.0" encoding="Windows-1252" ?>
<NotepadPlus>
    <AutoComplete language="awk">
        <KeyWord name="ARGC" />
        <KeyWord name="ARGV" />
        <KeyWord name="atan2" func="yes" >
            <Overload retVal="{atan of y/x}" >
                <Param name="y" />
                <Param name="x" />
            </Overload>
        </KeyWord>
        <KeyWord name="BEGIN" />
        <KeyWord name="END" />
        <KeyWord name="break" />
        <KeyWord name="close" func="yes" >
            <Overload retVal="" >
                <Param name="file name" />
            </Overload>
        </KeyWord>
        <KeyWord name="continue" />
        <KeyWord name="cos" func="yes" >
            <Overload retVal="{cos of x}" >
                <Param name="x, radians" />
            </Overload>
        </KeyWord>
        <KeyWord name="delete" />
        <KeyWord name="do" />
        <KeyWord name="else" />
        <KeyWord name="exit" />
        <KeyWord name="exp" func="yes" >
            <Overload retVal="{e ^ x}" >
                <Param name="x" />
            </Overload>
        </KeyWord>
        <KeyWord name="fflush" func="yes" >
            <Overload retVal="" >
                <Param name="" />
            </Overload>
        </KeyWord>
        <KeyWord name="FILENAME" />
        <KeyWord name="FNR" />
        <KeyWord name="for" />
        <KeyWord name="FS" />
        <KeyWord name="function" />
        <KeyWord name="getline" func="yes" >
            <Overload retVal="{next record}" >
                <Param name="[s]" />
            </Overload>
        </KeyWord>
        <KeyWord name="gsub" func="yes" >
            <Overload retVal="{number of substitutions}" >
                <Param name="r {find what}" />
                <Param name="t {replace with}" />
                <Param name="s {string}" />
            </Overload>
        </KeyWord>
        <KeyWord name="if" />
        <KeyWord name="in" />
        <KeyWord name="index" func="yes" >
            <Overload retVal="{position of t in s}" >
                <Param name="s {string}" />
                <Param name="t {find what}" />
            </Overload>
        </KeyWord>
        <KeyWord name="int" func="yes" >
            <Overload retVal="{integer part of x}" >
                <Param name="x" />
            </Overload>
        </KeyWord>
        <KeyWord name="length" func="yes" >
            <Overload retVal="{number of characters/items in s}" >
                <Param name="s" />
            </Overload>
        </KeyWord>
        <KeyWord name="log" func="yes" >
            <Overload retVal="{natural log of x}" >
                <Param name="x" />
            </Overload>
        </KeyWord>
        <KeyWord name="match" func="yes" >
            <Overload retVal="{position of r in s, RSTART, RLENGTH}" >
                <Param name="s {string}" />
                <Param name="r {find what}" />
            </Overload>
        </KeyWord>
        <KeyWord name="next" />
        <KeyWord name="NF" />
        <KeyWord name="NR" />
        <KeyWord name="OFMT" />
        <KeyWord name="OFS" />
        <KeyWord name="ORS" />
        <KeyWord name="print" func="yes" >
            <Overload retVal="" >
                <Param name="s" />
            </Overload>
        </KeyWord>
        <KeyWord name="printf" func="yes" >
            <Overload retVal="" >
                <Param name="format" />
                <Param name="[... {arguments}]" />
            </Overload>
        </KeyWord>
        <KeyWord name="rand" func="yes" >
            <Overload retVal="{0 <= random number <= 1}" >
                <Param name="<none>" />
            </Overload>
        </KeyWord>
        <KeyWord name="return" />
        <KeyWord name="RLENGTH" />
        <KeyWord name="RS" />
        <KeyWord name="RSTART" />
        <KeyWord name="sin" func="yes" >
            <Overload retVal="{sin of x}" >
                <Param name="x, radians" />
            </Overload>
        </KeyWord>
        <KeyWord name="split" func="yes" >
            <Overload retVal="{number of fields}" >
                <Param name="s {input str}" />
                <Param name="a {output arr}" />
                <Param name="fs {field separator}" />
            </Overload>
        </KeyWord>
        <KeyWord name="sprintf" func="yes" >
            <Overload retVal="{formatted string}" >
                <Param name="format" />
                <Param name="[... {arguments}]" />
            </Overload>
        </KeyWord>
        <KeyWord name="sqrt" func="yes" >
            <Overload retVal="{square root of x}" >
                <Param name="x" />
            </Overload>
        </KeyWord>
        <KeyWord name="srand" func="yes" >
            <Overload retVal="{new seed for rand()}" >
                <Param name="[x]" />
            </Overload>
        </KeyWord>
        <KeyWord name="sub" func="yes" >
            <Overload retVal="{number of substitutions}" >
                <Param name="r {find what}" />
                <Param name="t {replace with}" />
                <Param name="s {string}" />
            </Overload>
        </KeyWord>
        <KeyWord name="SUBSEP" />
        <KeyWord name="substr" func="yes" >
            <Overload retVal="{substring of s from p}" >
                <Param name="s {string}" />
                <Param name="p {start position}" />
                <Param name="[n {length}]" />
            </Overload>
        </KeyWord>
        <KeyWord name="system" func="yes" >
            <Overload retVal="{exit status}" >
                <Param name="command" />
            </Overload>
        </KeyWord>
        <KeyWord name="tolower" func="yes" >
            <Overload retVal="{lower-case string}" >
                <Param name="s" />
            </Overload>
        </KeyWord>
        <KeyWord name="toupper" func="yes" >
            <Overload retVal="{upper-case string}" >
                <Param name="s" />
            </Overload>
        </KeyWord>
        <KeyWord name="while" />
    </AutoComplete>
</NotepadPlus>