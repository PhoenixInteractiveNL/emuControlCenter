style "default"
{
  GtkTreeView::odd_row_color = "#E8E8E8"
  GtkTreeView::even_row_color = "#E8E8E8"

        fg[NORMAL]              = "#000000"
        fg[ACTIVE]              = "#000000"
        fg[INSENSITIVE]         = "#565248"
        fg[PRELIGHT]            = "#000000"
        fg[SELECTED]            = "#000000"
        
          bg[ACTIVE]              = "#e9eaec"
	  bg[NORMAL]              = "#e9eaec"
